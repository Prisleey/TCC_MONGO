                $.ajax({
                    url: "checarSaldo",
                    type: "post",
                    data: {id_usuario: id_usuario},
                    datatype: 'json',
                    success: function (result) {
                        $("#carteiraDisplay").html("R$" + result.creditos.toFixed(2));
                    },
                    error: function (data) {
                        alert("Erro ao consultar a carteira. ");
                    }
                });
            }
        });

